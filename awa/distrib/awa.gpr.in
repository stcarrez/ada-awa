with "util";
with "util_config";
with "el";
with "ado";
with "asf_core";

project AWA is
   Version := "@AWA_VERSION@";

   for Library_Name use "awa";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/awa");
   for Library_ALI_Dir use Util_Config.Alidir & "/awa/";
   for Externally_Built use "true";
end AWA;
